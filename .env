# Server Configuration
PORT=3000
NODE_ENV=development

# Storage Paths
UPLOAD_DIR=uploads
TILES_DIR=tiles

# File Upload Limits
MAX_FILE_SIZE=104857600
ALLOWED_FORMATS=image/jpeg,image/png,image/tiff,image/webp

# Tile Generation Settings
TILE_SIZE=256
TILE_OVERLAP=1
TILE_FORMAT=jpeg
TILE_QUALITY=80

# CORS Settings
CORS_ORIGIN=*
