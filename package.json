  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/multer": "^2.0.0",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/node": "^22.18.12",
