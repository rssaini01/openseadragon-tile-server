{
  "name": "openseadragon-tile-server",
  "version": "1.0.0",
  "description": "High-performance tile server for OpenSeadragon with Sharp",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "clean": "rimraf dist",
    "test": "jest --coverage",
    "prepare": "husky"
  },
  "keywords": [
